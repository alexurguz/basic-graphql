
mutation AddPersonToCourse2($course: ID!, $person: ID!) {
  addPeople(courseID: $course, personID: $person) {
    _id
    title
  }
}

query getCourse2($course: ID!) {
  getCourse(id: $course) {
    _id
    title
    description
    teacher
  }
}



